<div class="row g-3">
  <% for (let booking of bookings) { %>

    <div class="col-4">
      <div class="card p-1">
        <h5 class="card-title">#<%= booking._id%></h5>
        <div class="card-text">
          <div>Start Date: <%= booking.bookingStartDate %> </div>
          <div>End Date: <%= booking.bookingEndDate %> </div>
          <div>Transaction amount: $<%= booking.totalCharge %></div>
        </div>
      </div>
    </div>


  <% } %>

 <%- include("./bookingPagination.ejs", { startDate, endDate, currentPage, lastPage }) %>

</div>