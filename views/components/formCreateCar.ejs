<h2>Add a new car to fleet</h2>
<form action="/createCar" method="POST">
  <div>
    <label class="form-label">
      Service start year:
      <input class="form-control" type="text" name="startYear" />
    </label>
  </div>

  <div>
    <label class="form-label">
      Current mileage:
      <input class="form-control" type="text" name="milage" />
    </label>
  </div>

  <div>
    <label class="form-label">
      Car make:
    </label>
    <select name="make" id="carMake">
      <% for (let make of allMakes) { %>
        <option value=<%= make %>> <%= make %> </option>
      <% } %>
    </select>
  </div>

  <div>
      <label class="form-label">
        Car model:
      </label>
      <select name="model" id="carModel">
        <% for (let model of allModels) { %>
        <option value=<%= model %>> <%= model %> </option>
      <% } %>
      </select>
  </div>

  <div>
      <label class="form-label">
        Current rental branch:
      </label>
      <select name="currentRentalBranch" id="rentalBranch">
        <% for (let branch of allRentalBranches) { %>
        <option value="<%= branch %>"> <%= branch %> </option>
      <% } %>
      </select>
  </div>
  <br/>
  <button class="btn btn-primary" type="submit">Create</button>
</form>
